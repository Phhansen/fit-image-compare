{
  "name": "ptx-image-comparison",
  "version": "2.0.1",
  "description": "Image comparison slider component",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "^0.9.4",
    "@stencil/postcss": "0.0.4",
    "autoprefixer": "^8.5.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^22.2.3",
    "jest": "^22.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phhansen/ptx-image-compare"
  },
  "author": "Poul H. Hansen <poul.h.hansen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Phhansen/ptx-image-compare"
  },
  "homepage": "https://github.com/Phhansen/ptx-image-compare",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
