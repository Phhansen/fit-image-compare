{
  "name": "fit-image-compare",
  "version": "0.1.7",
  "description": "Image comparison slider component",
  "main": "dist/fit-image-compare.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/fit-image-compare.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "0.7.1",
    "@stencil/postcss": "0.0.4",
    "autoprefixer": "^8.1.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phhansen/fit-image-compare"
  },
  "author": "Poul H. Hansen <poul.h.hansen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Phhansen/fit-image-compare"
  },
  "homepage": "https://github.com/Phhansen/fit-image-compare",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
